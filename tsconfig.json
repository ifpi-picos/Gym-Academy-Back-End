{
	"compilerOptions": {
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"target": "ES2022",
		"lib": ["ES2023"],
		"sourceMap": true,
		"outDir": "dist",
		"strict": true,
		"noImplicitAny": true,
		"strictFunctionTypes": true,
		"noImplicitThis": true,
		"resolveJsonModule": true,
		"alwaysStrict": true,
		"removeComments": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		
	},
	
}